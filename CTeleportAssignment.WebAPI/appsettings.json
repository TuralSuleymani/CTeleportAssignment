{
  "Provider": {
    "Url": "url should be here"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      { "name": "Console" },
      {
        "name": "File",
        "Args": {
          "path": "./logs/log-.json",
          "rollingInterval": "Day",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      },
      {
        "name": "Seq",
        "serverUrl": "http://localhost:5341"
      }
    ]
  },
  "AllowedHosts": "*"
}
